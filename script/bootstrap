#!/bin/sh

echo "Bootstrapping dotfiles..."
ln -s ~/dev/dotfiles/git ~/.gitconfig
ln -s ~/dev/dotfiles/gitignore ~/.gitignore_global
ln -s ~/dev/dotfiles/vim ~/.vimrc
ln -s ~/dev/dotfiles/zsh ~/.zshrc
ln -s ~/dev/dotfiles/gem ~/.gemrc
ln -s ~/dev/dotfiles/pry ~/.pryrc
ln -s ~/dev/dotfiles/rvm ~/.rvmrc

echo "Done"
